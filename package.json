{
  "name": "velais-client-dashboard",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently -n client,server -c blue,green \"bun run dev:client\" \"bun run dev:server\"",
    "dev:client": "vite",
    "dev:server": "bun --watch server/dev.ts",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "bunx biome check .",
    "format": "bunx biome check --fix ."
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.3",
    "@tailwindcss/vite": "^4.2.0",
    "@types/bun": "latest",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-mkcert": "^1.17.10"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@hono/node-server": "^1.19.9",
    "@tanstack/react-query": "^5.90.21",
    "@workos-inc/authkit-react": "^0.16.0",
    "@workos-inc/node": "^8.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gsap": "^3.14.2",
    "hono": "^4.12.0",
    "lucide-react": "^0.575.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "2.15.4",
    "tailwind-merge": "^3.5.0"
  }
}
